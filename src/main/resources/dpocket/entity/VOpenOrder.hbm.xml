<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="dpocket.entity.VOpenOrder" table="vw_open_order" catalog="daigou">  
        <composite-id name="id" class="dpocket.entity.VOpenOrderId">  
            <key-property name="orderDate" type="timestamp">  
                 <column name="order_date" length="19" />
            </key-property>  
            <key-property name="orderId" type="java.lang.Integer">  
                <column name="order_id" />  
            </key-property>  
            <key-property name="name" type="java.lang.String">  
                <column name="name" />  
            </key-property>  
            <key-property name="amount" type="big_decimal">  
                <column name="amount" precision="8" not-null="true" />
            </key-property>  
            <key-property name="trackNumber" type="java.lang.String">  
                <column name="tracking_numbers" />  
            </key-property>  
            <key-property name="companyName" type="java.lang.String">  
                <column name="company_name" />  
            </key-property>  
            <key-property name="shippedDate" type="timestamp">  
                <column name="shipped_date" length="19"/>  
            </key-property>  
        </composite-id>  
    </class>  
</hibernate-mapping>
